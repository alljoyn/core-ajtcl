[project]
	description = AllJoyn Thin Core
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	read = group GitHub Replication
[access "refs/for/*"]
	pushMerge = group co-ajtcl-committers
[access "refs/for/refs/heads/*"]
	push = group co-ajtcl-committers
[access "refs/for/refs/heads/feature/*"]
	pushMerge = group Non-Interactive Users
	pushMerge = group co-ajtcl-committers
[access "refs/heads/*"]
	abandon = group co-ajtcl-committers
	create = group co-ajtcl-committers
	forgeAuthor = group co-ajtcl-committers
	label-Code-Review = -2..+2 group co-ajtcl-committers
	removeReviewer = group co-ajtcl-committers
	submit = group co-ajtcl-committers
	editTopicName = group co-ajtcl-committers
[access "refs/tags/*"]
	pushTag = group co-ajtcl-committers
	pushSignedTag = group co-ajtcl-committers
